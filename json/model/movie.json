{ 
    "properties": {
        "movie_id": { 
            "description": "The movie ID",
            "column name": "movie ID",
            "type": "string"
        },
        "run_id": {
            "description": "The run ID",
            "column name": "run ID", 
            "type": "string",
            "pop-up window": "movies on run run_id"
        },
        "dataset_id": {
            "description": "The ID by which the movie file(s) are known in SMRT Link",
            "column name": "dataset ID",
            "type": "integer",
            "comment": "Users can use this ID to find a movie in SMRT Link to view or delete it"
        },
        "date_str": {
            "description": "The time at which the movie was created",
            "column name": "date", 
            "type": "string"
        },
        "user_name": {
            "description": "The name of user who submitted the movie", 
            "column name": "submitted by", 
            "type": "string"
        },
        "instrument_name": {
            "description": "\"Sequel II\" or \"Revio\"",
            "column name": "instrument", 
            "type": "string"
        },
        "sample_type": {
            "description": "\"HiFi\" (default), \"Iso-Seq\", \"MAS-Seq\", or \"Amplicons\"",
            "column name": "sample type", 
            "type": "string"
        },
        "sample_name": {
            "description": "The name of the sample",
            "column name": "sample name", 
            "type": "string"
        },
        "num_shares": {
            "description": "The number of access rules granting access to this movie's path",
            "column name": "shares", 
            "type": "integer",
            "pop-up window": "users with access to movie movie_id"
        },
        "num_analyses": {
            "description": "The number of analyses associated with this movie",
            "column name": "analyses", 
            "type": "integer",
            "pop-up window": "analyses associated with movie movie_id"
        },
        "backup_device": {
            "description": "The name of the backup device, if any, else \"None\"",
            "column name": "backup device", 
            "type": "string"
        }
    }
}